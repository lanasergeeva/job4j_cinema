<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
    <filter-name>AuthFilter</filter-name>
    <filter-class>cinema.filter.AuthFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>AuthFilter</filter-name>
    <url-pattern>*.html</url-pattern>
  </filter-mapping>

  <servlet>
    <servlet-name>MovieServlet</servlet-name>
    <servlet-class>cinema.servlet.MovieServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>cinema.servlet.UserServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AuthServlet</servlet-name>
    <servlet-class>cinema.servlet.AuthServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetMovieServlet</servlet-name>
    <servlet-class>cinema.servlet.GetMovieSerlvet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TicketServlet</servlet-name>
    <servlet-class>cinema.servlet.TicketServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>MovieServlet</servlet-name>
    <url-pattern>/mov.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/user.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AuthServlet</servlet-name>
    <url-pattern>/auth.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetMovieServlet</servlet-name>
    <url-pattern>/movid.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TicketServlet</servlet-name>
    <url-pattern>/tick.do</url-pattern>
  </servlet-mapping>

</web-app>
